{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "functions": {
    "vercel-api.ts": {
      "runtime": "@vercel/node@5.3.24"
    }
  },
  "routes": [
    {
      "src": "^/api(?:/(.*))?$",
      "dest": "/vercel-api"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
