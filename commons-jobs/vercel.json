{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "functions": {
    "vercel-api.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "^/api(?:/(.*))?$",
      "dest": "/vercel-api.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
